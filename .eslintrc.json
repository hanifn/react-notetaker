{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": false,
        "node": true
    },
    "extends": "airbnb",
    "globals": {
        "describe": true,
        "it": true
    },
    rules: {
        "indent": ["error", 2],
        "no-console": 0,
        "no-undef": 0,
        "semi": 0,
        "comma-dangle": 0,
        "no-param-reassign": 0,
        "max-len": 0,
        "new-cap": 0,
        "arrow-body-style": 0
    }
}
